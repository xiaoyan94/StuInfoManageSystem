<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean" >
	<class name="Admin" table="t_admin" >
		<id name="adminId"  >
			<generator class="native"></generator>
		</id>
		<property name="adminPasswd" not-null="true"></property>
		<property name="adminTel"></property>
		<property name="adminName" not-null="true"></property>
		<property name="adminEmail"></property>
		<property name="adminRole"></property>
		<property name="sex"></property>
		<property name="adminRealName"></property>
	
	</class>
</hibernate-mapping>